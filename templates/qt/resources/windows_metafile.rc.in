#include "windows.h"

IDI_ICON1               ICON    DISCARDABLE     "@ICON_FILE@"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @CMAKE_PROJECT_VERSION_MAJOR@,@CMAKE_PROJECT_VERSION_MINOR@,@CMAKE_PROJECT_VERSION_PATCH@,0
 PRODUCTVERSION @CMAKE_PROJECT_VERSION_MAJOR@,@CMAKE_PROJECT_VERSION_MINOR@,@CMAKE_PROJECT_VERSION_PATCH@,0
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS VS_FF_DEBUG|VS_FF_PRERELEASE
#else
 FILEFLAGS 0L
#endif
 FILEOS VOS_NT_WINDOWS32
 FILETYPE 0x7L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904e4"
        BEGIN
            VALUE "CompanyName", "@CMAKE_CORP_NAME@\0"
            VALUE "FileDescription", "@APP_DESCRIPTION@\0"
            VALUE "FileVersion", "@CMAKE_PROJECT_VERSION@\0"
            VALUE "InternalName", "@TARGET_NAME@ \0"
            VALUE "LegalCopyright", "Copyright (C) 2016-@CURRENT_YEAR@ @CMAKE_CORP_NAME@\0"
            VALUE "OriginalFilename", "@TARGET_NAME@.dll\0"
            VALUE "ProductName", "@PROJECT_NAME@\0"
            VALUE "ProductVersion", "@CMAKE_PROJECT_VERSION@\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 1252
    END
END
