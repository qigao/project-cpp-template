#include "winver.h"

IDI_ICON1               ICON    DISCARDABLE     "@ICON_FILE@"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @CMAKE_PROJECT_VERSION_MAJOR@,@CMAKE_PROJECT_VERSION_MINOR@,@CMAKE_PROJECT_VERSION_PATCH@
 FILEFLAGS 0x0L
 FILEFLAGSMASK 0x3fL
 FILEOS 0x00040004L
 FILETYPE 0x0L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "@CMAKE_CORP_NAME@"
            VALUE "FileDescription", "@APP_DESCRIPTION@"
            VALUE "FileVersion", "@CMAKE_PROJECT_VERSION@"
            VALUE "LegalCopyright", "@COPYRIGHT@"
            VALUE "InternalName", "@TARGET_NAME@"
            VALUE "OriginalFilename", "@TARGET_NAME@.exe"
            VALUE "ProductName", "@TARGET_NAME@"
            VALUE "ProductVersion", "@CMAKE_PROJECT_VERSION@"
        END
    END
END
