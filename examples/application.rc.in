IDI_ICON1   ICON    DISCARDABLE "@APPLICATION_ICON@"

1 VERSIONINFO
FILEVERSION ${CMAKE_PROJECT_VERSION_MAJOR},${CMAKE_PROJECT_VERSION_MINOR},${CMAKE_PROJECT_VERSION_PATCH},0
PRODUCTVERSION ${CMAKE_PROJECT_VERSION_MAJOR},${CMAKE_PROJECT_VERSION_MINOR},${CMAKE_PROJECT_VERSION_PATCH},0
FILEOS 0x4
FILETYPE 0x0
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName", "@APPLICATION_COMPANY_NAME@"
      VALUE "FileDescription", "@APPLICATION_DESCRIPTION@"
      VALUE "FileVersion", "@APPLICATION_VERSION@"
      VALUE "InternalName", "@APPLICATION_NAME@"
      VALUE "LegalCopyright", "@APPLICATION_COPYRIGHT@"
      VALUE "OriginalFilename", "@TARGET_NAME@.exe"
      VALUE "ProductName", "@APPLICATION_NAME@"
      VALUE "ProductVersion", "@CMAKE_PROJECT_VERSION@"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 0x04B0
  END
END
